@model MVCExample.Models.Register
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_LayoutGuest.cshtml";
}

<h2>Index</h2>
@using(Html.BeginForm())
{
<table>
    <tr><td>Enter Username</td><td>@Html.TextBoxFor(a => a.UserId)</td></tr>
    <tr><td>Enter Password</td><td>@Html.PasswordFor(a => a.Pass)</td></tr>
    <tr><td colspan="2"><input type="submit" name="btnsubmit" value="Login"  />  </td></tr>
    <tr><td colspan="2">@Html.ActionLink("New User Sign Up Here","Reg")</td></tr>

</table>
   @ViewBag.data




}